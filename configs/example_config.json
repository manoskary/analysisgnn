{
    "model": "HybridGNN",
    "num_layers": 3,
    "hidden_channels": 256,
    "out_channels": 128,
    "dropout": 0.3,
    "lr": 0.005,
    "weight_decay": 0.005,
    "batch_size": 64,
    "num_epochs": 50,
    "main_tasks": ["cadence", "localkey", "romanNumeral"],
    "add_beats": true,
    "add_measures": false,
    "feature_type": "cadence",
    "use_wandb": false,
    "gpus": "-1",
    "num_workers": 4,
    "subgraph_size": 500,
    "mt_strategy": "wloss",
    "cl_training": false,
    "use_transpositions": true,
    "random_split": false,
    "verbose": true,
    "use_edge_loss": false,
    "lambda_edge": 0.1,
    "lambda_dctn": 0.5,
    "lambda_featl": 0.1,
    "lambda_ewc": 2.0
}
